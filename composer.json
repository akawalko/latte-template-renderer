{
  "name": "akawalko/latte-template-renderer",
  "description": "Interface implementation for a class that renders tests/templates for string and PSR-7 response objects using Latte engine - a Next-Generation Templating System for PHP.",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Arkadiusz KawaÅ‚ko"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-tokenizer": "*",
    "latte/latte": "^3.0",
    "akawalko/template-renderer-interface": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "nyholm/psr7": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Akawalko\\LatteTemplateRenderer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Akawalko\\LatteTemplateRenderer\\Tests\\": "tests/"
    }
  }
}
